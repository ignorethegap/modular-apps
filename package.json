{
  "name": "modular-apps",
  "version": "1.0.0",
  "description": "Common ground for all modules and apps. In your own company you might break these out as separate repositories",
  "main": "none.js",
  "scripts": {
    "install:address-lib": "cd address-lib && npm install",
    "install:fast-frontend-development": "cd node_modules/fast-frontend-development && npm install",
    "install:salespartner-angular": "cd salespartner-angular && npm install",
    "install:ui-frame-lib": "cd ui-frame-lib && npm install",
    "install:user-state-lib": "cd user-state-lib && npm install",
    "prepublish": "npm run install:address-lib && npm run install:fast-frontend-development && npm run install:salespartner-angular && npm run install:ui-frame-lib && npm run install:user-state-lib",
    "karma-firefox": "karma start salespartner-angular/karma.conf.js --browsers Firefox",
    "test-once": "karma start salespartner-angular/karma.conf.js --single-run --log-level debug",
    "test": "exit 0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ignorethegap/modular-apps.git"
  },
  "keywords": [
    "module",
    "frontend",
    "javascript",
    "apps"
  ],
  "author": "Henrik Vendelbo",
  "license": "UNLICENSED",
  "bugs": {
